snippet let
	(let [${1}] ${2})

snippet deftest
	(deftest ${1:test-name} ${2})

snippet defn
	(defn ${1:name} [${2}] ${3:body})

snippet defnn
	(defn ${1:name}
		([${2:params}] ${3:body}))

snippet def
	(def ${1:symbol} ${2:value})

snippet for
	(for [${1}] ${2})

snippet do
	(do ${1})

snippet doseq
	(doseq [${1}] ${2})

snippet case
	(case ${1}
		${3:pattern} ${4:value}
		${2:default})
